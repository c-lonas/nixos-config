#
# The value should just be the name of the theme, and 
# omit the `.yaml` file extension name.


{
  "admin" = {
    base16-theme = "everforest-dark-hard";
  };

  "chase" = {
    base16-theme = "rose-pine-moon";
  };

  "kids" = {
    base16-theme = "dracula";
  };
}
